import 'package:freezed_annotation/freezed_annotation.dart';

part 'create_room_chat_request_model.freezed.dart';
part 'create_room_chat_request_model.g.dart';

@freezed
class CreateRoomChatRequestModel with _$CreateRoomChatRequestModel {
  factory CreateRoomRoomChatRequestModel({
    @Default(
        'https://lh3.googleusercontent.com/a/ACg8ocLpZDs6ksBQqGLbA725ZVNXmHoAU6d1sK6DHEJFdbZA-dPGweI=s96-c')
    String? image,
    List<String>? messenger,
    String? name,
    List<Map<String, dynamic>?>? participants,
    DateTime? createdAt,
    DateTime? updatedAt,
  }) = _CreateRoomChatRequestModel;

  factory CreateRoomRoomChatRequestModel.fromJson(Map<String, dynamic> json) =>
      _$CreateRoomChatRequestModelFromJson(json);
}
